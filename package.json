{
  "name": "type-graphql-demo",
  "version": "1.0.0",
  "description": "graphql-demo",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "rm -rf dist && npm run build",
    "build": "swc src -d dist -C module.type=commonjs -C jsc.parser.decorators=true -C jsc.parser.syntax=typescript -C jsc.transform.decoratorMetadata=true -C jsc.target=es2022",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/main.js",
    "serve": "rm -rf dist && npm run build && node dist/federation.js",
    "sub": "node dist/subs/subServer.js"
  },
  "author": "TsaoLun",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14"
  },
  "dependencies": {
    "@apollo/gateway": "^0.28",
    "apollo-server": "2.21.0",
    "class-validator": "^0.13.2",
    "graphql": "15.5.0",
    "node-fetch": "2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "ws": "^8.11.0",
    "yaml": "^2.1.3"
  }
}
